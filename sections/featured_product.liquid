<section class="featured_product">
    <h2 class="featured_porduct__heading">
        {{collection.title}}
    </h2>
    <div class="featured-product__items">
        {% for product in collection[section.settings.collection].products %}
            <p>{{ product.title }}</p>
        {% endfor %}
    </div>
</section>
{% schema %}
{
    "name":"Featured_Product",
    "settings":[
        {
            "type":"collection",
            "id":"colection",
            "label":"colection"
        }
    ],
    "presets":[
        {
            "name":"Featured_Product",
            "category":"Product delay"
        }
    ]
}
{% endschema %}